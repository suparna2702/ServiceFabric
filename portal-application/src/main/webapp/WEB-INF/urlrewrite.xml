<?xml version="1.0" encoding="utf-8"?>

    <!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite2.6.dtd">

<urlrewrite>

  <rule>
    <from>^/public/member/([0-9]+)$</from>
    <to type="forward">/member/memberExternalPublicProfileView.xhtml?mid=$1</to>
  </rule>

  <rule>
    <from>^/public/business/([0-9]+)$</from>
    <to type="forward">/business/businessExternalPublicProfileView.xhtml?oid=$1</to>
  </rule>
</urlrewrite>